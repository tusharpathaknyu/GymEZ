# GYMEZ AWS Environment Configuration
# Copy this to .env and fill in your AWS details after running aws-setup.sh

# AWS Configuration
AWS_REGION=us-east-1
AWS_USER_POOL_ID=
AWS_USER_POOL_WEB_CLIENT_ID=
AWS_APPSYNC_GRAPHQL_ENDPOINT=
AWS_APPSYNC_REGION=us-east-1
AWS_APPSYNC_AUTHENTICATION_TYPE=AMAZON_COGNITO_USER_POOLS

# S3 Storage
AWS_S3_BUCKET_NAME=gymez-workout-videos
AWS_S3_REGION=us-east-1

# CloudFront CDN
CLOUDFRONT_DISTRIBUTION_ID=
CLOUDFRONT_DOMAIN_NAME=

# Analytics
PINPOINT_APP_ID=
PINPOINT_REGION=us-east-1

# API Configuration  
API_GATEWAY_URL=
WEBSOCKET_API_URL=

# App Configuration
APP_ENV=production
LOG_LEVEL=info

# Social Features
ENABLE_REAL_TIME_FEED=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_VIDEO_STREAMING=true

# Gym Features
ENABLE_CLASS_BOOKING=true
ENABLE_EQUIPMENT_TRACKING=true
ENABLE_PAYMENT_PROCESSING=true

# Premium Features
ENABLE_AI_RECOMMENDATIONS=true
ENABLE_ADVANCED_ANALYTICS=true
ENABLE_PERSONAL_TRAINING=true

# External Integrations
STRIPE_PUBLIC_KEY=
GOOGLE_MAPS_API_KEY=
YOUTUBE_API_KEY=

# Monitoring & Alerts
CLOUDWATCH_LOG_GROUP=/aws/lambda/gymez
X_RAY_TRACING_ENABLED=true